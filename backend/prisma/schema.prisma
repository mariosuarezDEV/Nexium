generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}


// Informaci√≥n de la empresa
model Empresa {
  id Int @id @default(autoincrement()) @unique
  nombre String
  direccion String?
}


